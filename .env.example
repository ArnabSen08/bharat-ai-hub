# AWS Configuration
AWS_REGION=ap-south-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# Amazon Bedrock
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/bharat_ai_hub
DYNAMODB_TABLE_PREFIX=bharat-ai-

# API Configuration
API_BASE_URL=http://localhost:3000
API_KEY=your_api_key_here

# Authentication
COGNITO_USER_POOL_ID=your_user_pool_id
COGNITO_CLIENT_ID=your_client_id
JWT_SECRET=your_jwt_secret

# Storage
S3_BUCKET_NAME=bharat-ai-hub-storage
S3_REGION=ap-south-1

# Redis Cache
REDIS_URL=redis://localhost:6379

# External Services
OPENAI_API_KEY=optional_for_fallback
TWILIO_ACCOUNT_SID=for_sms_notifications
TWILIO_AUTH_TOKEN=for_sms_notifications

# Feature Flags
ENABLE_HEALTHCARE_MODULE=true
ENABLE_RETAIL_MODULE=true
ENABLE_RURAL_MODULE=true
ENABLE_LEARNING_MODULE=true
ENABLE_CONTENT_MODULE=true
ENABLE_COMMUNITY_MODULE=true

# Monitoring
CLOUDWATCH_LOG_GROUP=/aws/bharat-ai-hub
ENABLE_XRAY_TRACING=true

# Environment
NODE_ENV=development
PORT=3000
